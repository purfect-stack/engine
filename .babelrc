{
  "presets": [
    "@babel/preset-es2015",
    ["@babel/preset-stage-2", { "decoratorsLegacy": true }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-transform-async-to-generator",
    "transform-export-extensions",
    "@babel/plugin-transform-runtime"
  ]
}
